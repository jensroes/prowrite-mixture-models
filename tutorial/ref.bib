title = {Age of acquisition and word frequency effects in picture naming: a dual-task investigation},
url = {http://apps.webofknowledge.com/full\_record.do?product=UA\&search\_mode=MarkedList\&qid=3\&SID=Y2yfYaMYqfT42sy9dHr\&page=1\&doc=1\&colName=WOS},
